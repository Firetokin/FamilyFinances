>\r\n            </template>\r\n        </el-menu>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport bus from './bus.js';\r\nexport default {\r\n    data() {\r\n        return {\r\n            collapse: false,\r\n            items: [\r\n                {\r\n                    icon: 'el-icon-lx-home',\r\n                    index: 'pageHome',\r\n                    title: '系统首页'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-cascades',\r\n                    index: '1',\r\n                    title: '管理员管理',\r\n\t\t\t\t\tsubs: [\r\n\t\t\t\t\t    {\r\n\t\t\t\t\t        index: 'adminTable',\r\n\t\t\t\t\t        title: '管理员列表'\r\n\t\t\t\t\t    }\r\n\t\t\t\t\t]\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-copy',\r\n                    index: '2',\r\n                    title: '用户管理',\r\n\t\t\t\t\tsubs: [\r\n\t\t\t\t\t    {\r\n\t\t\t\t\t        index: 'userTable',\r\n\t\t\t\t\t        title: '用户列表'\r\n\t\t\t\t\t    }\r\n\t\t\t\t\t]\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-calendar',\r\n                    index: '3',\r\n                    title: '收入支出管理',\r\n                    subs: [\r\n                        {\r\n                            index: 'incomeType',\r\n                            title: '收入类型'\r\n                        },\r\n                        {\r\n                            index: 'payType',\r\n                            title: '支出类型'\r\n                        }\r\n                    ]\r\n                },\r\n                {\r\n                    icon: 'el-icon-pie-chart',\r\n                    index: 'databaseM',\r\n                    title: '数据库管理'\r\n                }\r\n            ]\r\n        };\r\n    },\r\n    computed: {\r\n        onRoutes() {\r\n            return this.$route.path.replace('/', '');\r\n        }\r\n    },\r\n    created() {\r\n        // 通过 Event Bus 进行组件间通信，来折叠侧边栏\r\n        bus.$on('collapse', msg => {\r\n            this.collapse = msg;\r\n            bus.$emit('collapse-content', msg);\r\n        });\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar {\r\n    display: block;\r\n    position: absolute;\r\n    left: 0;\r\n    top: 70px;\r\n    bottom: 0;\r\n    overflow-y: scroll;\r\n}\r\n.sidebar::-webkit-scrollbar {\r\n    width: 0;\r\n}\r\n.sidebar-el-menu:not(.el-menu--collapse) {\r\n    width: 250px;\r\n}\r\n.sidebar > ul {\r\n    height: 100%;\r\n}\r\n</style>\r\n"]}]}